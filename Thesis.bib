arxivId = {1305.0614},
author = {Aravena, M. and Murphy, E. J. and Aguirre, J. E. and Ashby, M. L. N. and Benson, B. A. and Bothwell, M. and Brodwin, M. and Carlstrom, J. E. and Chapman, S. C. and Crawford, T. M. and de Breuck, C. and Fassnacht, C. D. and Gonzalez, A. H. and Greve, T. R. and Gullberg, B. and Hezaveh, Y. and Holder, G. P. and Holzapfel, W. L. and Keisler, R. and Malkan, M. and Marrone, D. P. and McIntyre, V. and Reichardt, C. L. and Sharon, K. and Spilker, J. S. and Stalder, B. and Stark, A. A. and Vieira, J. D. and Weiss, A.},
