file = {:u/bacook/Documents/Mendeley Desktop/Prochaska et al. - 2011.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
language = {en},
month = oct,
number = {2},
pages = {91},
title = {{PROBING THE INTERGALACTIC MEDIUM/GALAXY CONNECTION. V. ON THE ORIGIN OF Ly$\alpha$ AND O VI ABSORPTION AT z < 0.2}},
url = {http://labs.adsabs.harvard.edu/adsabs/abs/2011ApJ...740...91P},
volume = {740},
year = {2011}
}
