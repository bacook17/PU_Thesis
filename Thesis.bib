arxivId = {1210.4048},
author = {Menanteau, Felipe and Sif\'{o}n, Crist\'{o}bal and Barrientos, L. Felipe and Battaglia, Nicholas and Bond, J. Richard and Crichton, Devin and Das, Sudeep and Devlin, Mark J. and Dicker, Simon and D\"{u}nner, Rolando and Gralla, Megan and Hajian, Amir and Hasselfield, Matthew and Hilton, Matt and Hincks, Adam D. and Hughes, John P. and Infante, Leopoldo and Kosowsky, Arthur and Marriage, Tobias A. and Marsden, Danica and Moodley, Kavilan and Niemack, Michael D. and Nolta, Michael R. and Page, Lyman A. and Partridge, Bruce and Reese, Erik D. and Schmitt, Benjamin L. and Sievers, Jon and Spergel, David N. and Staggs, Suzanne T. and Switzer, Eric and Wollack, Edward J.},
