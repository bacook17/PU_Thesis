mendeley-tags = {DM},
month = jul,
number = {2},
primaryClass = {astro-ph},
title = {{Where Is the Dark Matter?}},
url = {http://labs.adsabs.harvard.edu/adsabs/abs/1995ApJ...447L..81B},
